          a.list-group-item(href='/reference/yum_history') Yum History/Undo
          a.list-group-item(href='/reference/svn_add') SVN Add
      #polymer.tab-pane.fade
        ul.list-group
          li.list-group-item
            a(href='http://www.polymer-project.org/components/core-icons/demo.html') Icons
          li.list-group-item
            a(href='/reference/polymer_misc') Misc
