          li.list-group-item
            a(href='https://www.polymer-project.org/articles/concatenating-web-components.html') Compiling
      #php.tab-pane.fade
        ul.list-group
          li.list-group-item
            a(href='/reference/php_backtrace') Backtrace
          li.list-group-item
            a(href='/reference/php_cake_query_log') CakePHP 1.2 query log
      #misc.tab-pane.fade
        ul.list-group
          a.list-group-item(href='/reference/letsencrypt') Lets Encrypt
          a.list-group-item(href='/reference/stock_photos') Stock Photos
          a.list-group-item(href='http://www.w3.org/TR/html5/forms.html#valid-e-mail-address') Email Addresses
          a.list-group-item(href='/reference/chromebook') Chromebook Crouton
          a.list-group-item(href='/reference/asus_laptop') Asus Laptop
          a.list-group-item(href='/reference/gnome3_monitor_config') Gnome 3 Master Monitor
          a.list-group-item(href='/reference/evoulent_mouse_config') Evoluent Mouse config for Linux
          a.list-group-item(href='/reference/gource') Gource defaults
