extends ../../templates/default

block vars
  -var title = 'James J Nadeau | Reference'
  -var description = 'Things I\'m saving for easy reference.'

block content
  div
    // Nav tabs
    ul.nav.nav-pills(role='tablist')
      li.active
        a(href='#js', role='tab', data-toggle='tab') JS
      li
        a(href='#html', role='tab', data-toggle='tab') HTML
      li
        a(href='#git', role='tab', data-toggle='tab') Git
      li
        a(href='#shell', role='tab', data-toggle='tab') Shell
      li
        a(href='#polymer', role='tab', data-toggle='tab') Polymer
      li
        a(href='#php', role='tab', data-toggle='tab') PHP
      li
        a(href='#misc', role='tab', data-toggle='tab') Misc
    // Tab panes
    .tab-content.content
      #js.tab-pane.active.fade.in
        ul.list-group
          a.list-group-item(href='/reference/js_backtrace') Backtrace
          a.list-group-item(href='/reference/js_callback') Callback
          a.list-group-item(href='/reference/js_debounce') Debounce
          a.list-group-item(href='/reference/js_Angular') Angular
          a.list-group-item(href='/reference/jqm_google_analytics') jQuery Mobile Google Analytics
      #html.tab-pane.fade
        ul.list-group
          a.list-group-item(href='/reference/html_all') All the Elements!
          a.list-group-item(href='/reference/bootstrap_test') Bootstrap Test
      #git.tab-pane.fade
        ul.list-group
          a.list-group-item(href='/reference/git_flow') Flow
          a.list-group-item(href='/reference/git_autocomplete') Autocomplete
      #shell.tab-pane.fade
        ul.list-group
          a.list-group-item(href='/reference/ssh_tunnel') SSH Tunneling
          a.list-group-item(href='/reference/bash_profile') Bash Profile
