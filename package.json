{
  "name": "jamesjnadeau.com",
  "version": "0.0.5",
  "description": "my personal website",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "NODE_ENV=development npm run dev:webpack",
    "dev:dashboard": "NODE_ENV=development webpack-dashboard -- npm run dev:webpack",
    "dev:webpack": "webpack-dev-server --progress --colors -d --host 0.0.0.0",
    "watch": "webpack --progress --colors --watch -d",
    "build": "webpack --progress --colors -p -d",
    "lint": "eslint .",
    "stage": "dev_appserver.py --host 0.0.0.0 ./",
    "inlineCSS": "find ./built -type f -name '*.html' -exec critical {} --base ./built --inline > {} \\;",
    "deploy": "gcloud app deploy app.yaml --project quantum-enigma-707",
    "deploy:verbose": "npm run deploy -- --verbosity=info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesjnadeau/jamesjnadeau.com.git"
  },
  "author": "James Nadeau",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesjnadeau/jamesjnadeau.com/issues"
  },
  "homepage": "https://github.com/jamesjnadeau/jamesjnadeau.com#readme",
  "dependencies": {
    "async": "^1.5.0",
    "bootstrap": "^3.3.6",
    "copy-webpack-plugin": "^4",
    "critical": "^0.9.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "filewalker": "^0.1.2",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^1.7.0",
    "jade": "^1.11.0",
    "jquery": "^2",
    "less": "^2.7.1",
    "less-loader": "^2.2.2",
    "loader-utils": "^0.2.12",
    "marked": "^0.3.9",
    "package": "^1.0.1",
    "smoothstate": "^0.7.2",
    "static-site-loader": "0.0.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.6.0",
    "webpack": "^1",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "snyk": "^1.69.3"
  }
